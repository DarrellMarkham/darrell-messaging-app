{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
          "endOfLine": "auto"
      },
      { "usePrettierrc": true }
  ],
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "no-unused-vars": "warn",
    "array-bracket-spacing": "warn",
    "func-names": "off",
    "quotes": ["error", "single"],
    "comma-dangle": "off"
  }
}
